<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard1.3;netstandard1.5;net452;net461</TargetFrameworks>
    <Version>1.0.0</Version>
    <InformationalVersion>1.0.0</InformationalVersion>
  </PropertyGroup>
  <PropertyGroup Condition="'$(TargetFramework)'=='netstandard1.3'">
    <DefineConstants>NETSTANDARD13</DefineConstants>
    <RuntimeFrameworkVersion>1.3.0</RuntimeFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition="'$(TargetFramework)'=='netstandard1.5' Or '$(TargetFramework)'=='net461'">
    <PackageReference Include="System.Security.Cryptography.Cng" Version="4.3.0" />
    <PackageReference Include="System.Diagnostics.StackTrace" Version="4.3.0" />
  </ItemGroup>
</Project>